= link_to :back, root_path

h3 Report (Output):
p#total-pages-read.indent-1 = "Total Pages Read: #{@report['total_pages_read']}"
h3 By Category: 
ul 
  - @report['category_breakdown'].each do |category_number, category_info| 
    li.dewey_decimal_category.indent-1 = "#{category_info['name']}: #{category_info['pages_read']}"

